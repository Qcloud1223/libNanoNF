    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch OneBoxOneFuncOnce",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/OneBoxOneFuncOnce",
            "args": ["./build/lib_1.so", "p1"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Launch NFVDemo",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/NFVDemo",
            "args": ["sandbox", "~/Desktop/maccdc2011_00013_20110312202724.pcap"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}